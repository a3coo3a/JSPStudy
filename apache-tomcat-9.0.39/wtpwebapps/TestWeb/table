
-- 회원 테이블 생성 --
create table users (
    id VARCHAR2(50) PRIMARY KEY,
    pw VARCHAR2(50),
    name VARCHAR2(50),
    phoneNumber NUMBER(20,0),
    email VARCHAR2(100),
    basicAddress VARCHAR2(200),
    detailAddress VARCHAR2(200),
    regdate date DEFAULT sysdate
);


-- 게시판 테이블 생성 --
create table bbs (
    bno number(20,0) PRIMARY KEY,
    writer VARCHAR2(50),
    title VARCHAR2(100),
    content VARCHAR2(1000),
    regdate DATE DEFAULT sysdate
);

create SEQUENCE bbs_seq START with 1 INCREMENT by 1 NOCACHE;